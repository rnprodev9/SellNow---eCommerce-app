{
  "name": "SellNow",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean && npm i",
    "test": "ava",
    "test:watch": "ava --watch",
    "coverage": "nyc ava && nyc report --reporter=html && open coverage/index.html || xdg-open coverage/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82"
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "0.0.8",
    "apisauce": "^0.10.0",
    "axios": "^0.16.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "base-64": "^0.1.0",
    "firebase": "^4.1.3",
    "format-json": "^1.0.3",
    "lodash": "^4.17.2",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "moment": "^2.18.1",
    "querystring": "^0.2.0",
    "querystringify": "0.0.4",
    "ramda": "^0.23.0",
    "react": "16.0.0-alpha.3",
    "react-native": "0.43.1",
    "react-native-admob": "^1.3.0",
    "react-native-android-back-button": "^0.3.0",
    "react-native-animatable": "^1.1.1",
    "react-native-app-settings": "^1.0.2",
    "react-native-config": "^0.2.1",
    "react-native-device-info": "^0.10.0",
    "react-native-device-settings": "^1.0.0",
    "react-native-drawer": "^2.3.0",
    "react-native-facebook-login": "^1.5.0",
    "react-native-fcm": "^7.1.0",
    "react-native-fs": "^2.3.2",
    "react-native-geocoder": "^0.4.8",
    "react-native-gifted-chat": "0.1.x",
    "react-native-google-places-autocomplete": "^1.2.12",
    "react-native-google-signin": "^0.10.0",
    "react-native-i18n": "^1.0.0",
    "react-native-image-picker": "^0.26.3",
    "react-native-image-resizer": "^0.1.0",
    "react-native-image-zoom-viewer": "^2.0.5",
    "react-native-infinite-scroll-view": "^0.4.3",
    "react-native-keyboard-aware-scroll-view": "^0.2.8",
    "react-native-linear-gradient": "^2.0.0",
    "react-native-maps": "0.13.0",
    "react-native-prompt": "^1.0.0",
    "react-native-restart": "0.0.2",
    "react-native-router-flux": "^3.37.0",
    "react-native-scrollable-tab-view": "^0.6.5",
    "react-native-share": "^1.0.20",
    "react-native-snackbar": "^0.3.6",
    "react-native-star-rating": "^1.0.7",
    "react-native-swipe-list-view": "^0.4.1",
    "react-native-swiper": "^1.5.4",
    "react-native-tab-view": "0.0.59",
    "react-native-vector-icons": "^4.0.1",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-persist": "^4.1.0",
    "redux-saga": "^0.14.3",
    "reduxsauce": "0.4.1",
    "seamless-immutable": "^7.0.1"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "enzyme": "^2.6.0",
    "husky": "^0.13.1",
    "ignite-animatable": "^0.3.1",
    "ignite-i18n": "^0.1.1",
    "ignite-ir-boilerplate-2016": "^0.2.1",
    "ignite-maps": "^0.1.2",
    "ignite-vector-icons": "^0.2.1",
    "mockery": "^2.0.0",
    "nyc": "^10.1.2",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.4.0",
    "react-native-mock": "^0.3.1",
    "reactotron-apisauce": "^1.7.0",
    "reactotron-react-native": "^1.7.0",
    "reactotron-redux": "^1.7.0",
    "reactotron-redux-saga": "^1.7.0",
    "snazzy": "^6.0.0",
    "standard": "8.6.0"
  },
  "ava": {
    "files": [
      "Tests/**/*.js",
      "!Tests/Setup.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "react-native-mock/mock",
      "./Tests/Setup"
    ],
    "babel": "inherit"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
